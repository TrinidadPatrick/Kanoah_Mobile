{
  "expo": {
    "name": "Kanoah_Mobile",
    "slug": "Kanoah_Mobile",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.mynewapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.mynewapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAVDyfYAh_unbWuN6Fq6q1BPJ1GkkihmRM"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "36105a23-262d-4b13-9cc9-f0145ab91bb5"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
